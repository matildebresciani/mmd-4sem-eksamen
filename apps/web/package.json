{
    "name": "web",
    "version": "1.0.0",
    "description": "Website template for Payload",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "postbuild": "next-sitemap --config next-sitemap.config.cjs",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && yarn build && yarn start",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "ii": "cross-env NODE_OPTIONS=--no-deprecation yarn install",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation biome check .",
        "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation biome check . --fix",
        "format:check": "biome format",
        "format:fix": "biome format --fix",
        "typecheck": "tsc --noEmit",
        "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
        "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm yarn.lock && yarn install",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
    },
    "dependencies": {
        "@lexical/react": "^0.24.0",
        "@next/third-parties": "^15.5.4",
        "@payloadcms/db-mongodb": "3.58.0",
        "@payloadcms/db-postgres": "3.58.0",
        "@payloadcms/email-resend": "3.58.0",
        "@payloadcms/live-preview-react": "3.58.0",
        "@payloadcms/next": "3.58.0",
        "@payloadcms/payload-cloud": "3.58.0",
        "@payloadcms/plugin-form-builder": "3.58.0",
        "@payloadcms/plugin-nested-docs": "3.58.0",
        "@payloadcms/plugin-redirects": "3.58.0",
        "@payloadcms/plugin-search": "3.58.0",
        "@payloadcms/plugin-seo": "3.58.0",
        "@payloadcms/richtext-lexical": "3.58.0",
        "@payloadcms/storage-vercel-blob": "3.58.0",
        "@payloadcms/translations": "3.58.0",
        "@payloadcms/ui": "3.58.0",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.3",
        "@repo/biome-config": "workspace:*",
        "@tanstack/react-query": "^5.90.2",
        "@types/luxon": "^3.7.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cross-env": "^7.0.3",
        "geist": "^1.5.1",
        "graphql": "^16.11.0",
        "lexical": "^0.24.0",
        "lucide-react": "^0.545.0",
        "luxon": "^3.7.2",
        "motion": "^12.23.22",
        "next": "^15.5.7",
        "next-intl": "^3.26.5",
        "next-sitemap": "^4.2.3",
        "payload": "3.58.0",
        "payload-admin-bar": "^1.0.7",
        "prism-react-renderer": "^2.4.1",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hook-form": "7.64.0",
        "sharp": "0.32.6",
        "slugify": "^1.6.6",
        "swiper": "^11.2.10",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.25.76",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@repo/typescript-config": "workspace:*",
        "@tailwindcss/postcss": "4.1.14",
        "@tailwindcss/typography": "^0.5.19",
        "@types/escape-html": "^1.0.4",
        "@types/node": "22.5.4",
        "@types/react": "19.0.7",
        "@types/react-dom": "19.0.3",
        "copyfiles": "^2.4.1",
        "postcss": "^8.5.6",
        "sass": "^1.93.2",
        "tailwindcss": "4.1.14",
        "typescript": "5.7.3"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0"
    }
}
